%% Date: 07/26/2018
%Visualization of particle motion with predefined functions

t=linspace(0,5,100);
x=0.6*t.^3-2*t.^2-8*t+12;
v=1.8*t.^2-4*t-8;
a=3.6*t-4;
subplot(3,1,1);
plot(t,x);
title({'PARTICLE MOTION','Assignment 3, Problem 2 - Tsu'});
ylabel('Position (m)');
grid on;
subplot(3,1,2);
plot(t,v);
ylabel('Velocity (m/s)');
grid on;
subplot(3,1,3);
plot(t,a);
xlabel('Time, t(s)');
ylabel('Acceleration (m/s^2)');
grid on;
