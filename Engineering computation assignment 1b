x1=input('input x1: ');
y1=input('input y1: ');
z1=input('input z1: ');
x2=input('input x2: ');
y2=input('input y2: ');
z2=input('input z2: ');

d=(((x2-x1)^2)+((y2-y1)^2)+((z2-z1)^2))^(1/2);
fprintf('The distance between (%d', x1);
fprintf(' ,%d', y1);
fprintf(' ,%d', z1);
fprintf(') and (%d',x2);
fprintf(' ,%d',y2);
fprintf(' ,%d', z2);
fprintf(') is %.2f', d);
